{
    "name": "armincms/nova-translation",
    "description": "Nova implementation of the `armincms/database-localization`.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-translation-loader": "^2.7",
        "gobrightspot/nova-detached-actions": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\NovaTranslation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [ 
                "Armincms\\NovaTranslation\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "2.0.0"
}
